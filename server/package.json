{
  "name": "ecommerce-server",
  "version": "1.0.0",
  "description": "Backend API for e-commerce platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "migrate": "sequelize-cli db:migrate",
