    "multer": "^1.4.5-lts.1",
    "@google-cloud/storage": "^7.7.0",
    "razorpay": "^2.9.2",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "nodemailer": "^6.9.8",
    "swagger-ui-express": "^5.0.0",
